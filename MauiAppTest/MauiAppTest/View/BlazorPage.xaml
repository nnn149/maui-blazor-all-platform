<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MauiAppTest.View"
             xmlns:razoribraryTest="clr-namespace:RazoribraryTest;assembly=RazoribraryTest"
             x:Class="MauiAppTest.View.BlazorPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}">

    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type razoribraryTest:Main}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>